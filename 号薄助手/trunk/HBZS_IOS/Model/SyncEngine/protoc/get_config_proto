option optimize_for = LITE_RUNTIME;

package ctuab.proto.message;

//Get system configuration response
message GetConfigResponse {
    optional uint32 session_expires = 1; 				//Session expires time£¨Measure: second£©
    optional string auth_url = 2; 						//Authentication URL
    optional int32 tpnool_version = 3; 					//TPNOOL version
    optional string tpnool_url = 4; 					//URL for downloading TPNOOL data
    optional string upload_portrait_url = 5;			//URL for uploading portrait
    optional string download_portrait_url = 6; 			//URL for downloading portrait
    optional string upload_all_Url = 7; 				//URL for uploading all contacts and contact groups
    optional string get_contact_list_url = 8; 			//URL for getting contact and contact group list
    optional string download_contact_url = 9; 			//URL for downloading contact and contact group
    optional string sync_upload_contact_url = 10; 		//URL for sync-uploading contact and contact group
    optional string get_contact_list_version_url = 11; 	//URL for getting contact list version
    optional string slow_sync_url = 12;                 //URL for slow sync
    optional string sync_portrait_url = 13;             //URL for sync portrait
    optional string sync_sms_url = 14;                  //URL for sync sms
    optional string upload_sms_url = 15;                //URL for upload sms
    optional string download_sms_url = 16;              //URL for download sms
    optional string client_report_url = 17;             //URL for client to send its report
    optional string backup_calllog_url = 18;            //URL for backup calllog
    optional string recover_calllog_url = 19;           //URL for recover calllog
    optional string storage_host_url = 20;              //URL for storage host
    optional string device_sign_url = 21;              //URL for device sign
    optional string plain_client_report_url = 22;       //URL for plain client to send its report
    optional string download_all_Url = 23; 				//URL for downloading all contacts and contact groups
    optional string get_user_cloud_summary_Url = 24;    //URL for cloud summary of cloud
    optional string query_client_version_Url = 25;      //URL for query client version
}
