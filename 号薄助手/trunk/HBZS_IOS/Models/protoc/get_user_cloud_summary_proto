option optimize_for = LITE_RUNTIME;

package ctuab.proto.message;

message GetUserCloudSummaryResponse {
	required int32 contactCount = 1;    //get contact count by cloud
	required int32 messageCount = 2;    //get message count by cloud
	required int32 callLogCount = 3;    //get call log count by cloud
}